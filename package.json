        "eslint": "^8.47.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.2",
        "ts-jest": "^29.1.1",
        "typedoc": "^0.24.8",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "discord.js": "^14.13.0",
        "pskv": "^1.1.3"
